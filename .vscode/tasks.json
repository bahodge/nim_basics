{
  "version": "2.0.0",
  "taskName": "Run module.nim",
  "command": "nim",
  "args": [
    "c",
    "-o:bin/${fileBasenameNoExtension}",
    "-r",
    "${fileBasename}"
  ],
  "options": {
    "cwd": "${workspaceRoot}"
  },
  "type": "shell",
  "group": {
    "kind": "build",
    "isDefault": true
  }
}